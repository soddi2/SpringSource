import lombok.extern.slf4j.Slf4j;

@Slf4j
@RestController
public class BookRestController {

	@Autowired
	private BookService serivce;
	
	@GetMapping("/list")
	public void list() {
		log.info("도서 목록 가져오기");
		
	}
	
}
